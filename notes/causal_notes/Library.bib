@misc{balakrishnanFundamentalLimitsStructureAgnostic2023,
  title = {The {{Fundamental Limits}} of {{Structure-Agnostic Functional Estimation}}},
  author = {Balakrishnan, Sivaraman and Kennedy, Edward H. and Wasserman, Larry},
  year = {2023},
