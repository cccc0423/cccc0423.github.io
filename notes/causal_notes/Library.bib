@article{zhaoEffectHealthCheckUps2022a,
  title = {The {{Effect}} of {{Health Check-Ups}} on {{Health Among}} the {{Elderly}} in {{China}}: {{Evidence From}} 2011--2018 {{Longitudinal Data}}},
