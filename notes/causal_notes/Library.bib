@article{zhouEqualizationSelectionReassessing2019,
  title = {Equalization or {{Selection}}? {{Reassessing}} the ``{{Meritocratic Power}}'' of a {{College Degree}} in {{Intergenerational Income Mobility}}},
  shorttitle = {Equalization or {{Selection}}?},
  author = {Zhou, Xiang},
